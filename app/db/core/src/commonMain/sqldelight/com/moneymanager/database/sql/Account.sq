CREATE TABLE Account (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    openingDate INTEGER NOT NULL
);

selectAll:
SELECT * FROM Account ORDER BY name;

selectById:
SELECT * FROM Account WHERE id = ?;

insert:
INSERT INTO Account(name, openingDate)
VALUES (?, ?);

update:
UPDATE Account
SET name = ?
WHERE id = ?;

delete:
DELETE FROM Account WHERE id = ?;

lastInsertRowId:
SELECT last_insert_rowid();
